{
    "builds": [
      {
        "src": "elevate-frontend/next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "elevate-backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/elevate-backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  